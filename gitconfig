[color]
	diff = auto
	status = auto
	branch = auto
[core]
	excludesfile = ~/.gitignore_global
[alias]
	a     = add
	ap    = add -p
	b     = branch
	c     = commit
	ca    = commit -a
	co    = checkout
	cm    = commit -m
	cam   = commit -am
	pom   = push origin master
	unstage = reset HEAD --
	last  = log -1 HEAD
	st    = status
	br    = branch
	d     = diff
	hist  = log --pretty=format:'%h %ai | %s%d [%an]' --graph --date=short --all
